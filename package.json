{
	"name": "@carlos.araujo.aus/n8n-nodes-verify-email",
	"version": "1.0.3",
	"description": "Verify Email Node for n8n",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Carlos Araujo",
		"email": "carlos.araujo.aus@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/carlos-araujo-aus/n8n-nodes-verify-email.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/VerifyEmailApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/VerifyEmailProgrammatic/VerifyEmailProgrammatic.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^24.10.1",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"rimraf": "^6.1.2"
	}
}
